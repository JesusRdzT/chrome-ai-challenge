 {
	"manifest_version": 3,
	"name": "Google AI Challenge Extension",
	"version": "1.0.0",
	"action": {
		"default_popup": "popup/index.html",
		"default_icon": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
		}
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
  	},
	"background": {
		"service_worker": "background/index.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": ["https://*/*", "http://*/*"],
			 "css": ["content/index.css"],
			"js": [
				 "content/index.js"
			 ]
		}
	],
	"permissions": [
     "scripting",
     "activeTab",
     "contextMenus",
     "aiLanguageModelOriginTrial",
     "storage",
     "tts",
     "ttsEngine",
	 "tabs"
   ],
   "host_permissions": [
    "<all_urls>"
  	]
   
}
